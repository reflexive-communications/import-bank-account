<?xml version="1.0"?>
<extension key="import-bank-account" type="module">
    <file>import_bank_account</file>
    <name>Import Contributions (bank account)</name>
    <description>Import contributions by a contact's bank account number or any other custom field</description>
    <license>AGPL-3.0</license>
    <maintainer>
        <author>Sandor Semsey</author>
        <email>sandor@es-progress.hu</email>
    </maintainer>
    <urls>
        <url desc="Main Extension Page">https://github.com/reflexive-communications/import-bank-account</url>
        <url desc="Documentation">https://github.com/reflexive-communications/import-bank-account</url>
        <url desc="Support">https://github.com/reflexive-communications/import-bank-account/issues</url>
        <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
    </urls>
    <releaseDate>2022-03-19</releaseDate>
    <version>1.0.4</version>
    <develStage>stable</develStage>
    <compatibility>
        <ver>5.0</ver>
    </compatibility>
    <classloader>
        <psr4 prefix="Civi\" path="Civi"/>
    </classloader>
    <civix>
        <namespace>CRM/ImportBankAccount</namespace>
        <angularModule>crmImportBankAccount</angularModule>
    </civix>
</extension>
